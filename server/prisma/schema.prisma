generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  password  String
  firstName String
  lastName  String
  phone     String?
  address   String?
  userType  UserType @default(VISITOR)
}

enum UserType {
  STUDENT
  TEACHER
  SUPER_ADMIN
  ADMIN
  PARENT
  VISITOR
}
